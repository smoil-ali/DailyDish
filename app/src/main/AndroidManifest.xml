<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.reactive.dailydish">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.DailyDish">
        <activity android:name=".Acitivities.SplashActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".Acitivities.CustomerActivity" />
        <activity android:name=".Acitivities.KitchenActivity" />
        <activity android:name=".Acitivities.RegisterActivity" />
        <activity android:name=".Acitivities.DashBoard"/>
        <activity android:name=".Acitivities.SignInActivitye" />
        <activity android:name=".MainActivity" />
    </application>

</manifest>